{
    "codeLens.enable": false,
    "diagnostic.virtualText": false,
    "diagnostic.virtualTextLineSeparator": ">> ",
    "clangd.compilationDatabaseCandidates": [
        "${workspaceFolder}/build"
    ],
    "git.addGBlameToVirtualText": false,
    "git.enableGutters": true,
    "git.enableGlobalStatus": true,
    "suggest.detailField": "preview",
    "suggest.floatConfig": {
        "border": true,
    },
    "hover.floatConfig": {
        "border": true,
    },
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.useQuickfixForLocations": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.bracketEnterImprove": true,
    "explorer.icon.source": "builtin",
    "explorer.icon.enableNerdfont": true,
    "tsserver.enable": true,
    "typescript.format.semicolons": "remove",
    "typescript.showUnused": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.suggest.paths": true,
    "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
    "typescript.suggest.enabled": true,
    "typescript.suggestionActions.enabled": true,
    "typescript.suggest.autoImports": true,
    "javascript.autoClosingTags": true,
    "javascript.format.semicolons": "remove",
    "vetur.completion.autoImport": true,
    "vetur.completion.tagCasing": "initial",
    "vetur.experimental.templateInterpolationService": true,
    "vetur.languageFeatures.updateImportOnFileMove": true,
    "vetur.validation.interpolation": true,
    "vetur.validation.template": false,
    "vetur.validation.templateProps": true,
    "vetur.format.enable": true,
    "vetur.format.options.tabSize": 2,
    "vetur.format.defaultFormatter.css": "prettier",
    "vetur.format.defaultFormatter.js": "prettier",
    "vetur.format.defaultFormatter.html": "prettier",
    "vetur.format.defaultFormatterOptions": {
        "js-beautify-html": {
            "wrap_attributes": "force-expand-multiline"
        },
        "prettier": {
            "trilingComma": "all",
            "jsxBracketSameLine": false,
            "simi": false,
            "singleQuote": true
        },
        "prettyhtml": {
            "printWidth": 240,
            "singleQuote": false,
            "wrapAttributes": false,
            "sortAttributes": false
        }
    },
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.updates.prompt": false,
    "rust-analyzer.completion.postfix.enable": true,
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "json.format.enable": true,
    "json.schemas": [
        {
            "fileMatch": [
                "manifest.json"
            ],
            "url": "https://json.schemastore.org/chrome-manifest"
        }
    ],
    "languageserver": {
        "golang": {
            "enable": true,
            "command": "gopls",
            "rootPatterns": [
                "go.mod",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "go"
            ],
            "initializationOptions": {
                "usePlaceholders": true,
                "directoryFilters": [],
                "completeUnimported": true
            }
        },
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": [
                "dockerfile"
            ],
            "args": [
                "--stdio"
            ]
        },
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ]
        },
        "vue": {
            "command": "vls",
            "enable": false,
            "args": [],
            "filetypes": [
                "vue"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "cmake": {
            "command": "cmake-language-server",
            "filetypes": [
                "cmake"
            ],
            "rootPatterns": [
                "CMakeLists.txt",
                "build/"
            ],
            "initializationOptions": {
                "buildDirectory": "build"
            }
        },
        "vimls": {
            "command": "vim-language-server",
            "args": [
                "--stdio"
            ],
            "initializationOptions": {
                "isNeovim": true,
                "iskeyword": "@,48-57,_,192-255,-#",
                "vimruntime": "",
                "runtimepath": "",
                "diagnostic": {
                    "enable": true
                },
                "indexes": {
                    "runtimepath": true,
                    "gap": 100,
                    "count": 3,
                    "projectRootPatterns": [
                        "strange-root-pattern",
                        ".git",
                        ".vimrc",
                        "autoload",
                        "plugin"
                    ]
                },
                "suggest": {
                    "fromVimruntime": true,
                    "fromRuntimepath": true
                }
            },
            "filetypes": [
                "vim"
            ]
        }
    }
}
