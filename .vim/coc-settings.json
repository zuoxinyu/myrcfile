{
    "codeLens.enable": true,
    //"diagnostic.virtualText": true,
    "git.virtualTextPrefix": "     ",
    //"git.addGBlameToVirtualText": true,
    "git.enableGutters": true,
    //"coc-swagger.enabled": true,
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.preferences.colorSupport": true,
    "coc.preferences.snippets.enable": false,
    "coc.preferences.previewAutoClose": true,
    "coc.preferences.enableFloatHighlight": true,
    //"coc.preferences.formatOnInsertLeave": true,
    "coc.preferences.useQuickfixForLocations": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.enableVimDevicons": true,
    "coc.preferences.enableMarkdown": true,
    "coc.preferences.bracketEnterImprove": true,
    //"eslint.enable": true,
    //"eslint.filetypes": [
    //    "javascript",
    //    "javascriptreact",
    //    "typescript",
    //    "typescriptreact"
    //],
    "vetur.format.enable": true,
    "vetur.completion.autoImport": true,
    "vetur.completion.tagCasing": "initial",
    "vetur.experimental.templateInterpolationService": true,
    "vetur.validation.interpolation": false,
    "vetur.validation.template": false,
    "tsserver.enable": true,
    "typescript.format.semicolons": "remove",
    "typescript.updateImportsOnFileMove.enable": true,
    "typescript.showUnused": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.suggest.paths": true,
    "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
    "typescript.suggest.enabled": true,
    "typescript.suggestionActions.enabled": true,
    "typescript.suggest.autoImports": true,
    "rust-analyzer.hoverActions.enable": true,
    "rust-analyzer.hoverActions.linksInHover": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.inlayHints.refreshOnInsertMode": true,
    "rust-analyzer.inlayHints.chainingHintsSeparator": "â€£",
    "rust-analyzer.lens.run": true,
    "rust-analyzer.lens.implementations": true,
    "rust-analyzer.updates.prompt": false,
    "rust-analyzer.callInfo.full": true,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.cargo.autoreload": true,
    "rust-analyzer.completion.addCallParenthesis": true,
    "rust-analyzer.completion.addCallArgumentSnippets": true,
    "rust-analyzer.lens.methodReferences": false,
    "rust-analyzer.completion.postfix.enable": true,
    "json.format.enable": true,
    "python.jediEnabled": false,
    "languageserver": {
        "golang": {
            "enable": false,
            "command": "gopls",
            "rootPatterns": [
                "go.mod",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "go"
            ],
            "initializationOptions": {
                "usePlaceholders": true,
                "directoryFilters": [
                    "-face",
                    "-videod",
                    "-archer"
                ],
                "completeUnimported": true
            }
        },
        "ccls": {
            "command": "ccls",
            "progressOnInitialization": true,
            "rootPatterns": [
                ".ccls",
                "compile_flags.txt",
                "compile_commands.json",
                "build/compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": ["c", "cpp", "cc", "h", "hpp"],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                },
                "highlight": {
                    "lsRanges": true
                }
            }
        },
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": [
                "dockerfile"
            ],
            "args": [
                "--stdio"
            ]
        },
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "vue": {
            "command": "vls",
            "enable": false,
            "args": [],
            "filetypes": [
                "vue"
            ],
            "ignoredRootPaths": [
                "~"
            ]

        },
        "efm": {
            "command": "efm-langserver",
            "enable": false,
            "args": [],
            "filetypes": [
                "vim",
                "eruby",
                "markdown"
            ]
        }
    }
}
