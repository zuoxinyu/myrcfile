    "keybindings": [
        // Application-level Keys
        {
            "command": "closeWindow",
            "keys": "alt+f4"
        },
        {
            "command": "toggleFullscreen",
            "keys": "f11"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "ctrl+shift+space"
        },
        {
            "command": "openSettings",
            "keys": "ctrl+,"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        // Tab Management
        // "command": "closeTab" is unbound by default.
        //   The closeTab command closes a tab without confirmation, even if it has multiple panes.
        {
            "command": "newTab",
            "keys": "alt+shift+t"
        },
        {
            "command": "duplicateTab",
            "keys": "alt+shift+d"
        },
        {
            "command": "nextTab",
            "keys": "ctrl+tab"
        },
        {
            "command": "prevTab",
            "keys": "ctrl+shift+tab"
        },
        {
            "command": {
                "action": "newTab",
                "index": 0
            },
            "keys": "alt+shift+1"
